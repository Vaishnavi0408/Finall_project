@model IEnumerable<Finall_project.Models.LoginTab>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.pass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.role)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.pass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.role)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.uname }) |
            @Html.ActionLink("Details", "Details", new { id=item.uname }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.uname })
        </td>
    </tr>
}

</table>
